{
  "manifest_version": 2,
  "homepage_url": "https://tab.gladly.io",
  "description": "Donate to a cause you care about just by surfing the web!",
  "chrome_url_overrides": {
    "newtab": "iframe.html"
  },
  "icons": {
    "16": "./img/logo16.png",
    "32": "./img/logo32.png",
    "48": "./img/logo48.png",
    "128": "./img/logo128.png"
  },
  "name": "Tab for a Cause",
  "permissions": [
    "tabs"
  ],
  "version": "5.9"
}
